	<queryString>
		<![CDATA[SELECT
     lit_vat_journal_v."ad_client_id" AS lit_vat_journal_v_ad_client_id,
     lit_vat_journal_v."ad_org_id" AS lit_vat_journal_v_ad_org_id,
     lit_vat_journal_v."c_invoice_id" AS lit_vat_journal_v_c_invoice_id,
     lit_vat_journal_v."documentno" AS lit_vat_journal_v_documentno,
     lit_vat_journal_v."dateacct" AS lit_vat_journal_v_dateacct,
     lit_vat_journal_v."vatdocumentno" AS lit_vat_journal_v_vatdocumentno,
     lit_vat_journal_v."lit_vatjournal_name" AS lit_vat_journal_v_name,
     lit_vat_journal_v."taxindicator" AS lit_vat_journal_v_c_tax_taxindicator,
     lit_vat_journal_v."c_tax_name" AS lit_vat_journal_v_c_tax_name,
     lit_vat_journal_v."c_tax_description" AS lit_vat_journal_v_c_tax_description,

 lit_vat_journal_v.taxamt as  lit_vat_journal_v_c_invoicetax_taxamt,
     
   
     lit_vat_journal_v."taxbaseamtfull" AS lit_vat_journal_v_c_invoicetax_taxbaseamt,
     lit_vat_journal_v."c_bpartner_name" AS lit_vat_journal_v_c_bpartner_name,
     lit_vat_journal_v."taxid" AS lit_vat_journal_v_lit_c_bpartner_taxidinfo_lit_vatnumber,
     lit_vat_journal_v."c_tax_id" AS lit_vat_journal_v_c_tax_id,
     notaxamt,
     taxbaseamtnotax
FROM
     "adempiere"."lit_vat_journal_v" lit_vat_journal_v

WHERE
     lit_vat_journal_v."ad_org_id" = $P{AD_Org_ID} and
CASE WHEN   $P{isdate} ='N' then
 LIT_VAT_Period_ID=  $P{LIT_VAT_Period_ID}  
else 
lit_vat_journal_v."dateacct" >= $P{DateFrom} AND
lit_vat_journal_v."dateacct" <= $P{DateTo} 
end and
  CASE WHEN $P{LIT_IsExcludeLIPE}='Y' then  lit_vat_journal_v."c_tax_isexcludedlipe" =  'N' 
  ELSE 1=1
  END

and
lit_vat_journal_v."lit_vatjournal_id" = $P{LIT_VATJournal_ID} 
ORDER BY
     lit_vat_journal_v."c_tax_id" ASC]]>
	</queryString>
