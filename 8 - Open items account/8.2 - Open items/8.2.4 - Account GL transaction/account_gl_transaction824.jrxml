		<queryString language="SQL">
			<![CDATA[select 
e.value,
 e.name ||' '|| bp.name  as name,
 sum(a2.amtacctcr-a2.amtacctdr) as amt
  from fact_acct a
inner join fact_acct a2 on  a2.record_id=a.record_id  and a2.ad_table_id=a.ad_table_id
inner join C_ElementValue e on a2.account_id=c_elementvalue_id
left join c_bpartner bp on  a.c_bpartner_id= bp.c_bpartner_id
where a.Fact_Acct_ID =$P{RECORD_ID}
and  a.dateacct=a2.dateacct
and a.fact_acct_id<>a2.fact_acct_id
group by e.name,e.value, bp.name]]>
		</queryString>
