	<queryString>
		<![CDATA[select
	lit_adorg_header_portrait_image($P{AD_CLIENT_ID}, $P{AD_Org_ID} ),
	lit_adorg_footer_portrait_image($P{AD_CLIENT_ID}, $P{AD_Org_ID} ),
	lit_date_pattern($P{LOCALE_CODE}) as datepattern,
	lit_datetime_pattern($P{LOCALE_CODE}) as datetimepattern,
	(
	select
		c_currency.iso_code
	from
		c_acctschema
	inner join c_currency on
		c_acctschema.c_currency_id = c_currency.c_currency_id
	where
		c_acctschema.ad_client_id = $P{AD_CLIENT_ID}
		and c_acctschema.ad_org_id in (0, $P{AD_Org_ID})),
	et.lv0_value,
	et.lv0_name,
	et.lv1_value,
	et.lv1_name,
	et.lv2_value,
	et.lv2_name,
	et.lv3_value,
	et.lv3_name,
	et.lv4_value,
	et.lv4_name,
	et.lv5_value,
	et.lv5_name,
	et.lv6_value,
	et.lv6_name,
	ce.value,
	ce.name,
	(case
		ce.accounttype when 'O' then 'L'
		else ce.accounttype
	end) as ACCOUNTTYPE,
	sum(fa.amtacctdr) as DARE,
	sum(fa.amtacctcr) as AVERE,
	sum(fa.amtacctdr-fa.amtacctcr) as SALDO,
	sum(case when fa.gl_category_id = 1000047 then (fa.amtacctdr-fa.amtacctcr) else 0 end) as BILAPP
from
	ad_treenode tn
inner join c_elementvalue ce on
	ce.c_elementvalue_id = tn.node_id
inner join lateral get_ev_tree($P{AD_CLIENT_ID}, $P{AD_Org_ID}) et on
	tn.parent_id = et.c_elementvalue_id
left join fact_acct fa
  on
	(fa.account_id = ce.c_elementvalue_id
		and fa.ad_client_id = $P{AD_CLIENT_ID}
		and fa.C_ACCTSCHEMA_ID = $P{C_AcctSchema_ID}
		and (fa.AD_ORG_ID = $P{AD_Org_ID}))
where
	tn.ad_client_id = $P{AD_CLIENT_ID}
	and ce.issummary = 'N'
	and fa.postingtype = 'A'
	and fa.dateacct between $P{DateAcct1} and $P{DateAcct2}
group by
	et.lv0_value,
	et.lv0_name,
	et.lv1_value,
	et.lv1_name,
	et.lv2_value,
	et.lv2_name,
	et.lv3_value,
	et.lv3_name,
	et.lv4_value,
	et.lv4_name,
	et.lv5_value,
	et.lv5_name,
	et.lv6_value,
	et.lv6_name,
	ce.value,
	ce.name,
	ce.accounttype
order by
	ce.value]]>
	</queryString>
