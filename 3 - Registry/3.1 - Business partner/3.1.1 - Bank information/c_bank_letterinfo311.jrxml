	<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
lit_adorg_header_portrait_image($P{AD_CLIENT_ID}, $P{AD_Org_ID} ),
	 lit_adorg_footer_portrait_image($P{AD_CLIENT_ID}, $P{AD_Org_ID} ),
	lit_datetime_pattern($P{LOCALE_CODE}) AS datetimepattern,
	c_greeting.greeting,
     c_bank."swiftcode" AS c_bank_swiftcode,
     c_bank."name" AS c_bank_name,
     c_bankaccount."name" AS c_bankaccount_name,
     c_bankaccount."iban" AS c_bankaccount_iban,
     c_bankaccount."accountno" AS c_bankaccount_accountno,
     c_bankaccount."isdefault" AS c_bankaccount_isdefault,
     c_bank."description" AS c_bank_description,
     c_bankaccount."description" AS c_bankaccount_description,
     c_location."address1" AS c_location_address1,
     c_location."city" AS c_location_city,
     c_location."postal" AS c_location_postal,
     c_location."regionname" AS c_location_regionname,
     c_bank."routingno" AS c_bank_routingno,
     c_bpartner."name" AS c_bpartner_name,
     c_bpartner."name2" AS c_bpartner_name2,
     c_bpartner."description" AS c_bpartner_description,
     c_bpartner_location."phone" AS c_bpartner_location_phone,
     c_bpartner_location."fax" AS c_bpartner_location_fax,
     c_bpartner_location."name" AS c_bpartner_location_name,
     c_bpartner_location."isbillto" AS c_bpartner_location_isbillto,
     c_bpartner_location."email" AS c_bpartner_location_email,
     c_location_BP."address1" AS c_location_A_address1,
     c_location_BP."address2" AS c_location_A_address2,
     c_location_BP."city" AS c_location_A_city,
     c_location_BP."postal" AS c_location_A_postal,
     c_location_BP."regionname" AS c_location_A_regionname,
     c_location_BP."address3" AS c_location_A_address3,
     coalesce(c_country_trl_BP.name, c_country_BP.name) as c_country_BP_name,
     c_location_ORG."address1" AS c_location_ORG_address1,
     c_location_ORG."address2" AS c_location_ORG_address2,
     c_location_ORG."address3" AS c_location_ORG_address3,
     c_location_ORG."regionname" AS c_location_ORG_regionname,
     c_location_ORG."city" AS c_location_ORG_city,
     c_location_ORG."postal" AS c_location_ORG_postal,
     coalesce(c_country_trl_ORG."name", c_country_ORG.name)  AS c_country_ORG_name,
     ad_org."description" AS ad_org_description,
     ad_orginfo."phone" AS ad_orginfo_phone,
     ad_orginfo."email" AS ad_orginfo_email,
     ad_orginfo."taxid" AS ad_orginfo_taxid
from ad_org
	inner join ad_client on ad_org.ad_client_id = ad_client.ad_client_id
    inner join c_bpartner on c_bpartner.ad_org_id = ad_org.ad_org_id
    left join c_greeting on c_greeting.c_greeting_id = c_bpartner.c_greeting_id
    left join c_bpartner_location c_bpartner_location ON c_bpartner."c_bpartner_id" = c_bpartner_location."c_bpartner_id"
    left join c_location c_location_BP ON c_bpartner_location."c_location_id" = c_location_BP."c_location_id"
    left join c_country c_country_BP on c_location_BP."c_country_id" = c_country_BP.c_country_id
    left join c_country_trl c_country_trl_BP ON c_country_BP."c_country_id" = c_country_trl_BP."c_country_id" and c_country_trl_BP.ad_language = $P{LOCALE_CODE}
    left join ad_orginfo ad_orginfo ON ad_org."ad_org_id" = ad_orginfo."ad_org_id"
    left join c_location c_location_ORG ON ad_orginfo."c_location_id" = c_location_ORG."c_location_id"
    left join c_country c_country_ORG on c_location_ORG."c_country_id" = c_country_ORG.c_country_id
    left join c_country_trl c_country_trl_ORG ON c_country_trl_ORG."c_country_id" = c_country_ORG."c_country_id" and c_country_trl_ORG.ad_language = $P{LOCALE_CODE}
    left join c_bank c_bank ON ad_org.ad_org_id = c_bank.ad_org_id 
    left join c_bankaccount c_bankaccount ON c_bank."c_bank_id" = c_bankaccount."c_bank_id"
    left join "adempiere"."c_location" c_location ON c_bank."c_location_id" = c_location."c_location_id"
where c_bpartner."c_bpartner_id" =   $P{C_BPartner_ID}
     and c_bankaccount."isdefault" = 'Y'
 AND c_bank."isdefault" = 'Y'
 AND c_bpartner_location."isbillto" = 'Y']]>
	</queryString>
