	lit_tables_exclusions.name as c_order_lit_exclusion_name,
	lit_tables_exclusions.description as c_order_lit_exclusion_description
from
	c_order
inner join ad_org on c_order.ad_org_id = ad_org.ad_org_id
inner join ad_orginfo on ad_org.ad_org_id = ad_orginfo.ad_org_id
inner join c_bpartner on c_order.c_bpartner_id = c_bpartner.c_bpartner_id
inner join c_currency on c_order.c_currency_id = c_currency.c_currency_id
left join c_orderline on c_order.c_order_id = c_orderline.c_order_id
left join m_product on c_orderline.m_product_id = m_product.m_product_id
left join c_bpartner_location on	c_bpartner.c_bpartner_id = c_bpartner_location.c_bpartner_id
left join c_location on	c_bpartner_location.c_location_id = c_location.c_location_id
left join c_country on c_location.c_country_id = c_country.c_country_id
left join c_country_trl on c_country.c_country_id = c_country_trl.c_country_id and c_country_trl.ad_language = $P{LOCALE_CODE}
left join m_warehouse on c_order.m_warehouse_id = m_warehouse.m_warehouse_id
left join c_location c_location_warehouse on m_warehouse.c_location_id = c_location_warehouse.c_location_id
left join c_country c_country_warehouse on c_location_warehouse.c_country_id = c_country_warehouse.c_country_id
left join c_country_trl c_country_trl_warehouse on c_country_warehouse.c_country_id = c_country_trl_warehouse.c_country_id and c_country_trl_warehouse.ad_language = $P{LOCALE_CODE}
left join ad_user on c_order.ad_user_id = ad_user.ad_user_id
left join c_paymentterm on c_order.c_paymentterm_id = c_paymentterm.c_paymentterm_id
left join c_paymentterm_trl on c_paymentterm.c_paymentterm_id = c_paymentterm_trl.c_paymentterm_id and c_paymentterm_trl.ad_language = $P{LOCALE_CODE} 
left join ad_ref_list ad_ref_list_deliveryviarule on c_order.deliveryviarule = ad_ref_list_deliveryviarule.value and ad_ref_list_deliveryviarule.ad_reference_id = 152
left join ad_ref_list_trl ad_ref_list_trl_deliveryviarule on ad_ref_list_deliveryviarule.ad_ref_list_id = ad_ref_list_trl_deliveryviarule.ad_ref_list_id and ad_ref_list_trl_deliveryviarule.ad_language = $P{LOCALE_CODE}
left join ad_ref_list ad_ref_list_paymentrule on c_order.paymentrule = ad_ref_list_paymentrule.value and ad_ref_list_paymentrule.ad_reference_id = 195
left join ad_ref_list_trl ad_ref_list_trl_paymentrule on ad_ref_list_paymentrule.ad_ref_list_id = ad_ref_list_trl_paymentrule.ad_ref_list_id and ad_ref_list_trl_paymentrule.ad_language = $P{LOCALE_CODE}
left join c_doctype on c_order.c_doctypetarget_id = c_doctype.c_doctype_id
left join c_doctype_trl on c_doctype.c_doctype_id = c_doctype_trl.c_doctype_id and c_doctype_trl.ad_language = $P{LOCALE_CODE}
left join c_tax on c_orderline.c_tax_id = c_tax.c_tax_id
left join c_uom on c_uom.c_uom_id = m_product.c_uom_id
left join c_uom_trl on c_uom.c_uom_id = c_uom_trl.c_uom_id and c_uom_trl.ad_language = $P{LOCALE_CODE}
left join c_bpartner c_bpartner_dropship on c_order.dropship_bpartner_id = c_bpartner_dropship.c_bpartner_id
left join c_bpartner_location c_bpartner_location_dropship on c_order.dropship_location_id = c_bpartner_location_dropship.c_bpartner_location_id and c_bpartner_location_dropship.isbillto = 'Y'
left join c_location c_location_dropship on c_bpartner_location_dropship.c_location_id = c_location_dropship.c_location_id
left join c_country c_country_dropship on c_location_dropship.c_country_id = c_country_dropship.c_country_id
left join c_country_trl c_country_trl_dropship on c_country_dropship.c_country_id = c_country_trl_dropship.c_country_id and c_country_trl_dropship.ad_language = $P{LOCALE_CODE}
left join m_shipper on c_order.m_shipper_id = m_shipper.m_shipper_id
left join lit_tables lit_tables_freightcost on c_order.lit_freightcost_id = lit_tables_freightcost.lit_tables_id
left join lit_c_bank_v on c_order.ad_client_id = lit_c_bank_v.c_bank_ad_client_id and lit_c_bank_v.c_bank_ad_org_id = c_order.ad_org_id
left join c_project on c_order.c_project_id = c_project.c_project_id
left join m_product_po on c_orderline.c_bpartner_id = m_product_po.c_bpartner_id and m_product_po.m_product_id = m_product.m_product_id
left join lit_tables on lit_tables.lit_tables_id = c_order.lit_termsandconditions_id
left join lit_tables lit_tables_exclusions on c_order.lit_exclusion_id = lit_tables_exclusions.lit_tables_id
where
	c_order.c_order_id = $P{RECORD_ID}
order by
	c_orderline.line asc]]>
