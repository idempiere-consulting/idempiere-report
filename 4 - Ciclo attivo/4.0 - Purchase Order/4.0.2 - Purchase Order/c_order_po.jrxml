	c_location_warehouse.address1 as c_location_warehouse_address1,
	c_location_warehouse.postal as c_location_warehouse_postal,
	c_location_warehouse.regionname as c_location_warehouse_regionname,
	coalesce(c_country_trl_warehouse.name, c_country_warehouse.name) as c_country_warehouse_name,
	ad_user.name as ad_user_name,
	coalesce(c_paymentterm_trl.name, c_paymentterm.name) as c_paymentterm_name,
	coalesce(ad_ref_list_trl_deliveryviarule.name, ad_ref_list_deliveryviarule.name) as c_order_deliveryviarule,
	coalesce(ad_ref_list_trl_paymentrule.name, ad_ref_list_paymentrule.name) as c_order_paymentrule,
	c_orderline.discount as c_orderline_discount,
	c_order.description as c_order_description,
	coalesce(c_doctype_trl.name, c_doctype.name) as c_doctype_name,
	coalesce(c_doctype_trl.printname, c_doctype.printname) as c_doctype_printname,
